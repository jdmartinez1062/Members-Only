<h1>Sign up</h1>
    
    

<%=form_for @user do |f|%>
    <div>
    <%=render 'shared/users_error_messages' %>
    </div>
    <%=f.label :name, "Username"%>
    <%=f.text_field :name%>
    <%=f.label :email%>
    <%=f.email_field :email%>
    <%=f.label :password%>
    <%=f.password_field :password%>
    <%=f.label :password_confirmation%>
    <%=f.password_field :password_confirmation%>
    <%=f.submit "Create account"%>    

<% end %>




